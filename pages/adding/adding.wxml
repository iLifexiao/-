<!-- 添加功能 begin  -->
<view class="icon-box">
  <image src="../../images/codeStyle.png" style='width: 23px; height: 23px'></image>
  <text style='margin-left: 5px;'>加密方式</text>
</view>
<view class="weui-cells weui-cells_after-title">
  <block wx:for="{{addList}}" wx:key="*this">
    <navigator url="{{item.url}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
            <image src="{{item.iconPath}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
        </view>
        <view class="weui-cell__bd">{{item.title}}</view>
        <view class="weui-cell__ft weui-cell__ft_in-access">{{item.detail}}</view>
    </navigator>
  </block>
</view>
<!-- 添加功能 end  -->